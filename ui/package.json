{
  "name": "react-redux-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/melyourhero/react-redux-boilerplate"
  },
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=10.0.0",
    "npm": "^5.6.0"
  },
  "scripts": {
    "test": "better-npm-run test",
    "lint": "tslint './src/**/*.tsx' './src/**/*.ts' './types/**/*.ts' -t stylish",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "build": "better-npm-run build"
  },
  "moduleRoots": [
    "src/"
  ],
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/webpack.prod.config",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "webpack-dev-server --hot --config webpack/webpack.dev.config",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "mocha -r ts-node/register src/**/*.spec.ts",
      "env": {
        "NODE_ENV": "development",
        "TS_NODE_PROJECT": "./tsconfig.test.json"
      }
    }
  },
  "license": "MIT",
